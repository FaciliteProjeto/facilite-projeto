generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id String @id @default(uuid())
  name String
  email String @unique
  password String
  phone String
  cpf String @unique
  createdAt DateTime @map("created_at")

  @@map("users")
}